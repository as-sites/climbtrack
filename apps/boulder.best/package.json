{
	"name": "@boulder.best/track",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"generate-favicon": "node scripts/generate-favicon.js"
	},
	"engines": {
		"node": "24.x"
	},
	"packageManager": "pnpm@10.27.0",
	"type": "module",
	"nx": {
		"name": "boulder.best",
		"targets": {
			"build": {
				"dependsOn": ["generate-favicon", "^build"]
			},
			"typecheck": {
				"executor": "@nx/js:tsc",
				"options": {
					"main": "./src/app/page.tsx",
					"outputPath": "./out-tsc",
					"tsConfig": "./tsconfig.json"
				}
			},
			"generate-favicon": {
				"executor": "nx:run-commands",
				"options": {
					"command": "node scripts/generate-favicon.js",
					"cwd": "apps/boulder.best"
				}
			}
		}
	},
	"dependencies": {
		"@hookform/resolvers": "catalog:",
		"zod": "catalog:",
		"next": "catalog:next",
		"react": "catalog:react",
		"react-dom": "catalog:react",
		"@boulder.best/shadcn": "workspace:^",
		"react-hook-form": "catalog:"
	},
	"devDependencies": {
		"favicons": "^7.2.0"
	}
}
